<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:titlebar="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <com.shu.messystem.component.TitleBar
            android:id="@+id/login_title_bar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            titlebar:BarBackgroundColor="@color/bg1"
            titlebar:RightTextColor="@color/white"
            titlebar:TitleNameCenter="设置" />

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:stretchColumns="1">

            <TableRow>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="10dip"
                    android:text="@string/seradd_textview" />

                <EditText
                    android:id="@+id/seradd_edittext"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:inputType="text"
                    android:labelFor="@+id/seradd_edittext"
                    android:padding="5dip"
                    android:text="@string/ser_addtext" />
            </TableRow>

            <TableRow>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="10dip"
                    android:text="@string/seruser_textview" />

                <EditText
                    android:id="@+id/seruser_edittext"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:inputType="textPassword"
                    android:labelFor="@+id/seruser_edittext"
                    android:padding="5dip"
                    android:text="@string/ser_usertext" />
            </TableRow>

            <TableRow>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="10dip"
                    android:text="@string/serpas_textview" />

                <EditText
                    android:id="@+id/serpas_edittext"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:inputType="textPassword"
                    android:labelFor="@+id/serpas_edittext"
                    android:padding="5dip"
                    android:text="@string/ser_pastext" />
            </TableRow>

            <TableRow>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="10dip"
                    android:text="更新地址:" />

                <EditText
                    android:id="@+id/update_addr"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:inputType="text"
                    android:labelFor="@+id/update_addr"
                    android:padding="5dip"
                    android:text="" />
            </TableRow>

            <TableRow>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="10dip"
                    android:text="更新信息文件:" />

                <EditText
